                            "body": "{\n  \"object\": \"list\",\n  \"url\": \"\/api\/v1\/domains\",\n  \"has_more\": true,\n  \"data\": [\n    {\n      \"domain_uuid\": \"4018f7a3-8e0a-47bb-9f4f-04b1313e0e1b\",\n      \"object\": \"domain\",\n      \"domain_name\": \"10001.fspbx.com\",\n      \"domain_enabled\": true,\n      \"domain_description\": \"BluePeak Solutions\"\n    }\n  ]\n}",
